{
  "rewrites": [
    { "source": "/Tantuka/:path*", "destination": "/:path*" },
    { "source": "/Tantuka", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.@(js|css|svg|png|jpg|jpeg|gif|ico|json)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
