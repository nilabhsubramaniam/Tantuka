{
    "name": "tantuka-frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "build:github": "set GITHUB_PAGES=true&& next build",
        "start": "serve out",
        "serve": "serve out",
        "lint": "next lint",
        "export": "next build && next export",
        "deploy": "set GITHUB_PAGES=true&& next build && touch out/.nojekyll && git add -f out && git commit -m \"Deploy to GitHub Pages\" && git subtree push --prefix out origin gh-pages"
    },
    "dependencies": {
        "autoprefixer": "^10.4.16",
        "axios": "^1.5.1",
        "framer-motion": "^12.23.24",
        "next": "14.0.0",
        "next-pwa": "^5.6.0",
        "postcss": "^8.4.31",
        "react": "^18",
        "react-dom": "^18",
        "react-intersection-observer": "^10.0.0",
        "react-query": "^3.39.3",
        "tailwindcss": "^3.3.3"
    },
    "devDependencies": {
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "eslint": "^8",
        "eslint-config-next": "14.0.0",
        "serve": "^14.2.5",
        "typescript": "^5"
    }
}
